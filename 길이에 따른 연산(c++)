#include <string>
#include <vector>

using namespace std;

int solution(vector<int> num_list) {
    int answer1 = 1,answer2 =0;
    if(num_list.size() <= 10){
        for(int n : num_list){
            answer1 *= n ;
        }
        return answer1;
    }
    else
        for(int n : num_list){
            answer2 += n;
        }
    return answer2;
}
