#include <string>
#include <vector>

using namespace std;

vector<int> solution(vector<string> strlist) {
    vector<int> answer;
    for(int i=0;i<strlist.size();i++){
        answer.push_back(strlist[i].size());
    }
    return answer;
}
=====================================================
#include <string>
#include <vector>

using namespace std;

vector<int> solution(vector<string> strlist) {
    vector<int> answer;
    for(auto str : strlist)
        answer.push_back(str.length());

    return answer;
}
